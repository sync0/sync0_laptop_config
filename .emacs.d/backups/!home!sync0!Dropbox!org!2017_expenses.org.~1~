#+LATEX_CLASS_OPTIONS: [paper=A4,landscape,twoside=false,twocolumn=true,headinclude=false,footinclude=false,fontsize=10,BCOR=0mm,DIV=15,pagesize=auto,titlepage=false,abstract=false,mpinclude=false,headings=normal]
